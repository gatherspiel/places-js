{
  "name": "@bponnaluri/places-js",
  "version": "0.0.29",
  "homepage": "https://github.com/free-gather/places-js#",
  "license": "GPL-3.0",
  "description": "A framework based on Web Components to build interactive UIs for in person event listings",
  "type": "module",
  "keywords": [
    "framework",
    "ui",
    "state"
  ],
  "main": "dist/places-js.umd.js",
  "module": "dist/places-js.esm.js",
  "unpkg": "dist/places-js.umd.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js --format esm --sourcemap true -o dist/places-js.esm.js",
    "updateLocal": "npm run build && yalc publish --push --replace"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gatherspiel/places-js.git"
  },
  "devDependencies": {
    "rollup": "^4.46.3",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.9.2"
  }
}
